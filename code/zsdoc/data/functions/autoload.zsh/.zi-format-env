local uspl2="$1" which="$2"
if [[ "$which" = "1" ]]; then
  typeset -a elem
  elem=( "${(z@)ZI[PATH__$uspl2]}" )
elif [[ "$which" = "2" ]]; then
  typeset -a elem
  elem=( "${(z@)ZI[FPATH__$uspl2]}" )
fi
local answer="" e
for e in "${elem[@]}"; do
  [[ -z "$e" ]] && continue
  e="${(Q)e}"
  answer+="$e"$'\n'
done
[[ -n "$answer" ]] && REPLY="$answer"
